      <blogpost id="1157" key="2be0ad34-181c-4034-93a8-b09e88c54623" parentID="1156" level="3" creatorID="0" sortOrder="0" createDate="2018-05-14T15:11:06" updateDate="2018-05-15T16:04:08" nodeName="Microsoft Dynamics NAV" urlName="microsoft-dynamics-nav" path="-1,1103,1156,1157" isDoc="" nodeType="1096" creatorName="dusan" writerName="dusan" writerID="0" template="1061" nodeTypeAlias="blogpost" isPublished="true">
